<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


    <servlet>
        <!--        注意两个name ,保持一致-->
        <servlet-name>RequestTestServlet</servlet-name>
        <servlet-class>com.RainbowSea.servlet.RequestTestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RequestTestServlet</servlet-name>
        <!--        注意 / 开始-->
        <url-pattern>/Request</url-pattern>
    </servlet-mapping>




    <servlet>
        <!--        注意两个name ,保持一致-->
        <servlet-name>AServlet</servlet-name>
        <servlet-class>com.RainbowSea.servlet.AServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AServlet</servlet-name>
        <!--        注意 / 开始-->
        <url-pattern>/A</url-pattern>
    </servlet-mapping>

    <servlet>
        <!--        注意两个name ,保持一致-->
        <servlet-name>BServlet</servlet-name>
        <servlet-class>com.RainbowSea.servlet.BServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BServlet</servlet-name>
        <!--        注意 / 开始-->
        <url-pattern>/b</url-pattern>
    </servlet-mapping>


</web-app>